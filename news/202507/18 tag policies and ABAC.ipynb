   "source": [
    "CREATE OR REPLACE FUNCTION `202507`.mask_str(s STRING)\n",
    "RETURNS STRING\n",
    "RETURN IF(\n",
    "          s IS NULL OR LENGTH(s) <= 1,\n",
    "          s,\n",
    "          CONCAT(LEFT(s, 1), '***', RIGHT(s, 1))\n",
    "       );"
   ]
  }
